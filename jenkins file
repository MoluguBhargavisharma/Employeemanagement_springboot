pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
                //bat "rmdir /s /q Employeemanagement_springboot"
                
                bat "git clone https://github.com/MoluguBhargavisharma/Employeemanagement_springboot.git"

                bat "mvn clean -f Employeemanagement_springboot"
            } 
                
        }
            stage('install'){
                steps{
                    bat "mvn install -f Employeemanagement_springboot"
                }
            }
            stage('test'){
                steps{
                    bat "mvn test -f Employeemanagement_springboot"
                }
            }
                stage('package'){
                  steps{
                      bat "mvn package -f Employeemanagement_springboot"
                    }
                }
     }
}
